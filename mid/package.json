{
  "name": "mid",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && webpack && tsc",
    "start": "ts-node ./src/server.ts",
    "test": "mocha",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "pretest": "npm run setup:db",
    "setup:db": "sqlite3 ./sql/dummy.db < ./sql/createSchema.sql && sqlite3 ./sql/dummy.db < ./sql/createData.sql"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "mocha": {
    "exit": true,
    "recursive": true,
    "require": "ts-node/register",
    "full-trace": true,
    "extension": [
      "ts"
    ],
    "timeout": 5000
  },
  "eslintConfig": {
    "root": true,
    "ignorePatterns": [
      "node_modules/**/*.js",
      "dist/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "@typescript-eslint/consistent-type-definitions": [
        "error",
        "interface"
      ],
      "@typescript-eslint/array-type": [
        "error",
        {
          "default": "array"
        }
      ],
      "@typescript-eslint/consistent-type-assertions": "warn",
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": [
            "variable"
          ],
          "format": [
            "camelCase",
            "UPPER_CASE",
            "PascalCase"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.175",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@types/sinon": "^10.0.4",
    "@types/supertest": "^2.0.11",
    "@types/webpack-dev-middleware": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "chai": "^4.3.4",
    "css-loader": "^6.4.0",
    "eslint": "^8.0.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.4.0",
    "mocha": "^9.1.3",
    "sinon": "^11.1.2",
    "style-loader": "^3.3.0",
    "supertest": "^6.1.6",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.4",
    "@mui/material": "^5.0.4",
    "@types/knex": "^0.16.1",
    "@types/sqlite3": "^3.1.7",
    "connect-history-api-fallback": "^1.6.0",
    "knex": "^0.95.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "sqlite3": "^4.2.0"
  }
}
